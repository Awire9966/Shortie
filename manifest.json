{
    "manifest_version": 3,
    "name": "Shortie",
    "version": "1.0.0",
    "description": "A converter to convert youtube shorts UIs to Regular UIs.",
    "icons": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    },
    "action": {
      "default_popup": "home.html",
      "default_icon": {
        "16": "icon/icon16.png",
        "24": "icon/icon24.png",
        "32": "icon/icon32.png"
      }
    },
    "permissions": [
      "activeTab" 
    ]
    
  }